{
    "window.zoomLevel": 1,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "workbench.colorTheme": "Zenburn Dark Matter - Deep Sea - Aquamarine",
    "workbench.iconTheme": "vscode-icons",
    "glassit.alpha": 240,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#f5ae77"
                }
            },
            {
                "scope": "entity.name.function.preprocessor",
                "settings": {
                    "foreground": "#4bbaaa"
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#c2e6ff"
                }
            },
            {
                "scope": "keyword.operator.new",
                "settings": {
                    "foreground": "#543dff"
                }
            },
            {
                "scope": "keyword.operator.delete",
                "settings": {
                    "foreground": "#543dff"
                }
            },
            {
                "scope": "variable.other.global",
                "settings": {
                    "foreground": "#fffae2ce"
                }
            },
            {
                "scope": "variable.other.property",
                "settings": {
                    "foreground": "#a09898"
                }
            },
            {
                "scope": [
                    "comment.line",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#058650"
                }
            },
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#146745"
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#85e0bc"
                }
            },
            {
                "scope": "variable.other.enummember",
                "settings": {
                    "foreground": "#4f65d2ca"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#5cd9ffce"
                }
            },
            {
                "scope": "entity.name.function.operator",
                "settings": {
                    "foreground": "#f48e8ed0"
                }
            },
            {
                "scope": "entity.name.function.member",
                "settings": {
                    "foreground": "#ffb2e1"
                }
            }
        ],
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.accessibilitySupport": "off",
    "editor.formatOnType": true,
    "editor.suggest.preview": true,
    "files.encoding": "shiftjis",
    "virtual-folders.folders": [
        {
            "name": "Headers",
            "files": [
                "**/*.h"
            ]
        },
        {
            "name": "Source",
            "files": [
                "**/*.cpp"
            ]
        }
    ],
    "files.exclude": {
        "x64": true,
        ".vs": true
    },
    "fileGroup.groups": {
        "baseClass": {
            "detail": "aaa",
            "files": [
                "${workspaceFolder:}/program/Base.cpp",
                "${workspaceFolder:}/program/Base.h",
                "${workspaceFolder:}/program/CardBase.cpp",
                "${workspaceFolder:}/program/CardBase.h"
            ]
        },
        "Cards": {
            "files": [
                "${workspaceFolder:}/program/ActionCard.cpp",
                "${workspaceFolder:}/program/ActionCard.h",
                "${workspaceFolder:}/program/EnvironmentCard.cpp",
                "${workspaceFolder:}/program/EnvironmentCard.h",
                "${workspaceFolder:}/program/ItemCard.cpp",
                "${workspaceFolder:}/program/ItemCard.h"
            ]
        },
        "Characters": {
            "files": [
                "${workspaceFolder:}/program/Player.cpp",
                "${workspaceFolder:}/program/Player.h",
                "${workspaceFolder:}/program/Npc.cpp",
                "${workspaceFolder:}/program/Npc.h"
            ]
        },
        "Game": {
            "files": [
                "${workspaceFolder:}/program/Game.cpp",
                "${workspaceFolder:}/program/Game.h",
                "${workspaceFolder:}/program/Main.cpp",
                "${workspaceFolder:}/program/Main.h"
            ]
        },
        "System": {
            "files": [
                "${workspaceFolder:}/program/Hit.cpp",
                "${workspaceFolder:}/program/Hit.h",
                "${workspaceFolder:}/program/Vector2.cpp",
                "${workspaceFolder:}/program/Vector2.h",
                "${workspaceFolder:}/program/Vector3.cpp",
                "${workspaceFolder:}/program/Vector3.h"
            ]
        }
    },
    "explorer.fileNesting.patterns":{
        "*.sln" : "${capture}.vcxproj,${capture}.vcxproj.filters"
    },
    "favorites.groupsFirst" : false
}